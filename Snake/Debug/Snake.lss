                                     ldi     r16,( 0 << COM0A1 | 0 << COM0A0 |\
0000a6 e000                                        0 << WGM01  | 0 << WGM00  )
0000a7 bd04                          out     TCCR0A,r16
                                 
                                     ldi     r16,( 0 << WGM02 | 0 << CS02 |\
0000a8 e003                                        1 << CS01  | 1 << CS00 )
0000a9 bd05                          out     TCCR0B,r16
                                     
0000aa e001                          ldi     r16,( 1 << TOIE0 )
0000ab 9300 006e                     sts     TIMSK0,r16
                                 
