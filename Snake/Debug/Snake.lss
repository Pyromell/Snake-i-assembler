                                     ; Prescaler 2..0: Set to 1/128 => 125 kHz
0000a0 e807                          ldi     r16,0b1000_0111
0000a1 9300 007a                     sts     ADCSRA,r16
                                 
                                     ; ADC Multiplexer Selection Register:
                                     ; REFS1..0, ADLAR, --, MUX3..0
